application:
	scanDirs: off
	mapping:
		*: App\*Module\Presenters\*Presenter

routing:
	routes:
		'[<module>/]<presenter>':
			presenter: Homepage
			action: default

http:
	csp:
		default-src: [self]
		script-src: [nonce]
		object-src: [none]
		base-uri: [none]

extensions:
	version: VitKutny\Version\Extension
